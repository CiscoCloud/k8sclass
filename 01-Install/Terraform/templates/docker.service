[Unit]
Description=Docker Application Container Engine
Documentation=http://docs.docker.io

[Service]
ExecStart=/usr/bin/docker daemon --iptables=false --ip-masq=false --host=unix:///var/run/docker.sock --log-level=error --storage-driver=overlay --bridge=cbr0
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
